<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
	<servlet-name>Login</servlet-name>
	<display-name>Login</display-name>
	<description></description>
	<servlet-class>com.nagarro.java.servlet.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UploadImageDetails</servlet-name>
		<display-name>UploadImageDetails</display-name>
		<description></description>
		<servlet-class>com.nagarro.java.servlet.UploadImageDetails</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Update</servlet-name>
		<display-name>Update</display-name>
		<description></description>
		<servlet-class>com.nagarro.java.servlet.Update</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UploadImageDetails</servlet-name>
		<url-pattern>/UploadImageDetails</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Update</servlet-name>
		<url-pattern>/Update</url-pattern>
	</servlet-mapping>
</web-app>
